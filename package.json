{
  "name": "ezlang",
  "version": "1.0.0",
  "description": "A simple programming language for Discord bot development with Python package integration",
  "main": "dist/index.js",
  "bin": {
    "ezlang": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && node dist/index.js repl",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:lexer": "ts-node src/test.ts",
    "test:runtime": "ts-node src/test-runtime.ts",
    "test:discord": "ts-node src/test-discord.ts",
    "start:repl": "npm run build && node dist/index.js repl",
    "test:repl": "npm run build && echo 'var x = 10\nvar y = 20\nprint(x + y)' | node dist/index.js repl",
    "test:cli": "./test-cli.sh",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "programming-language",
    "discord-bot",
    "interpreter",
    "typescript",
    "python-integration"
  ],
  "author": "EzLang Contributors",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "discord.js": "^14.14.1",
    "node-ipc": "^10.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.28",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
