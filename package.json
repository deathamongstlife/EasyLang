{
  "name": "ezlang",
  "version": "1.0.0",
  "description": "A simple programming language for Discord bot development with Python package integration",
  "main": "dist/index.js",
  "bin": {
    "ezlang": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && node dist/index.js repl",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:lexer": "ts-node scripts/test-lexer.ts",
    "test:runtime": "ts-node scripts/test-runtime.ts",
    "test:discord": "ts-node scripts/test-discord.ts",
    "start:repl": "npm run build && node dist/index.js repl",
    "test:repl": "npm run build && echo 'var x = 10\nvar y = 20\nprint(x + y)' | node dist/index.js repl",
    "test:cli": "./test-cli.sh",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "programming-language",
    "discord-bot",
    "interpreter",
    "typescript",
    "python-integration"
  ],
  "author": "EzLang Contributors",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "discord.js": "^14.14.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-ipc": "^10.1.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.8",
    "@types/node-ipc": "^9.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "prettier": "3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
