// FizzBuzz implementation
function fizzBuzz(n) {
    for i in range(1, n + 1) {
        var output = ""

        if i % 3 == 0 {
            output = "Fizz"
        }

        if i % 5 == 0 {
            output = output + "Buzz"
        }

        if output == "" {
            output = str(i)
        }

        print(output)
    }
}

print("FizzBuzz from 1 to 15:")
fizzBuzz(15)
